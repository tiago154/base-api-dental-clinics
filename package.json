{
  "name": "base-api-dental-clinics",
  "version": "0.0.1",
  "description": "Exemplo de uma API básica com todas as boas práticas e integrações",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "create:migrate": "knex --knexfile src/clients/database/config.ts migrate:make -x ts --",
    "start:migrate": "npx dotenv -e .env -- knex --knexfile src/clients/database/config.ts migrate:latest",
    "rollback:migrate": "npx dotenv -e .env -- knex --knexfile src/clients/database/config.ts migrate:rollback"
  },
  "keywords": [],
  "author": "Tiago Santos Da Silva",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "knex": "^0.95.14",
    "pg": "^8.7.1"
  }
}
